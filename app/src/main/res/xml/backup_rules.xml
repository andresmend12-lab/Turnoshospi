<?xml version="1.0" encoding="utf-8"?>
<full-backup-content>
    <!-- Excluir DataStore (preferencias y proto) para evitar restaurar estado obsoleto o sensible en otro dispositivo -->
    <exclude domain="file" path="datastore/" />
    <!-- Excluir SharedPreferences si contienen tokens temporales -->
    <exclude domain="sharedpref" path="." />
</full-backup-content>